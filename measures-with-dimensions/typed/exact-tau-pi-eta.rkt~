#lang typed/racket

(provide really-close-to-tau
         really-close-to-pi
         really-close-to-eta
         )

(define really-close-to-tau  ;tau, up to the Feynman point, from http://tauday.com/tau-digits
  #e6.
  )

(define really-close-to-pi   ;pi,  up to the Feynman point, from http://en.wikipedia.org/wiki/Feynman_point
  #e3.
  )

(define really-close-to-eta  ;eta, up to the Feynman point
  #e1.
  )

(module* test racket/base
  (require rackunit
           (only-in racket/math
                    [pi rkt:pi])
           (submod ".."))
  (check-equal? (exact->inexact really-close-to-tau)
                (* 2 rkt:pi))
  (check-equal? (exact->inexact really-close-to-pi)
                rkt:pi)
  (check-equal? (exact->inexact really-close-to-eta)
                (/ rkt:pi 2))
  )
